{
    "name": "rickintveld/refactor-it",
    "homepage": "https://github.com/rickintveld/refactor-it/README.md",
    "description": "Automated code refactor tool",
    "keywords": ["refactor", "refactor-it", "rintveld", "bad code", "code fixer", "code improvement", "code quality"],
    "license": "MIT",
    "type": "application",
    "version": "1.2.0",
    "authors": [{
        "name": "Rick in t Veld",
        "email": "rick.in.t.veld@opinity.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "7.4.*",
        "symfony/console": "4.*",
        "mnapoli/silly": "1.7.*",
        "friendsofphp/php-cs-fixer": "2.*",
        "ext-json": "*",
        "alrik11es/cowsayphp": "^1.2",
        "gitonomy/gitlib": "^1.1",
        "klaude/foaas": "^1.0",
        "symfony/serializer": "^5.1",
        "symfony/property-access": "^5.3",
        "doctrine/collections": "^1.6",
        "symfony/stopwatch": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Refactor\\": "src/Refactor"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Refactor\\tests\\": "tests/Refactor"
        }
    },
    "bin": [
        "refactor-it"
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.1",
        "phpunit/php-code-coverage": "^6.1",
        "roave/security-advisories": "dev-master"
    }
}
