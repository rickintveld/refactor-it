{
    "name": "rocket/refactor",
    "homepage": "https://github.com/rickone1/refactor-it/README.md",
    "description": "Automated code refactor tool",
    "keywords": ["refactor", "reindent", "rocket", "sync", "rsync"],
    "license": "MIT",
    "version": "1.0.0",
    "authors": [{
        "name": "Rick in t Veld",
        "email": "rick_91@live.nl"
    }],
    "require": {
        "php": ">=7.1.0",
        "symfony/console": "*",
        "mnapoli/silly": "*",
        "friendsofphp/php-cs-fixer": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Rocket\\": "src/Refactor"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rocket\\": "tests/Rocket"
        }
    },
    "bin": [
        "refactor"
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    }
}
