{
    "name": "rintveld/refactor-it",
    "homepage": "https://github.com/rickone1/refactor-it/README.md",
    "description": "Automated code refactor tool",
    "keywords": ["refactor", "reindent", "rocket", "sync", "rsync"],
    "license": "MIT",
    "version": "1.0.1",
    "authors": [{
        "name": "Rick in t Veld",
        "email": "rick.in.t.veld@opinity.nl"
    }],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "symfony/console": "4.2.*",
        "mnapoli/silly": "1.7.*",
        "friendsofphp/php-cs-fixer": "2.*",
        "ext-json": "*",
        "alrik11es/cowsayphp": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Refactor\\": "src/Refactor"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Refactor\\": "tests/Refactor"
        }
    },
    "bin": [
        "refactor-it"
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    }
}
